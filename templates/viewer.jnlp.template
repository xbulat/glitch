<?xml version="1.0" encoding="UTF-8"?>
<jnlp codebase="https://${host}" spec="1.0+">
<information>
  <title>Virtual Console Client</title>
  <vendor>Dell Inc.</vendor>
   <icon href="https://${host}/images/logo.gif" kind="splash"/>
   <shortcut online="true"/>
 </information>
<application-desc main-class="com.avocent.idrac.kvm.Main">
   <argument>ip=${host}</argument>
   <argument>vm=1</argument>
   <argument>title=${host}</argument>
   <argument>user=${user}</argument>
   <argument>passwd=${password}</argument>
   <argument>kmport=5900</argument>
   <argument>vport=5900</argument>
   <argument>apcp=1</argument>
   <argument>reconnect=2</argument>
   <argument>chat=1</argument>
   <argument>F1=1</argument>
   <argument>custom=0</argument>
   <argument>scaling=15</argument>
   <argument>minwinheight=100</argument>
   <argument>minwinwidth=100</argument>
   <argument>videoborder=0</argument>
   <argument>version=2</argument>
 </application-desc>
 <security>
   <all-permissions/>
 </security>
 <resources>
   <j2se version="1.7+"/>
   <jar href="https://${host}/software/avctKVM.jar" download="eager" main="true" />
 </resources>
 <resources os="Windows" arch="x86">
   <nativelib href="https://${host}/software/avctKVMIOWin32.jar" download="eager"/>
   <nativelib href="https://${host}/software/avctVMAPI_DLLWin32.jar" download="eager"/>
 </resources>
 <resources os="Windows" arch="amd64">
   <nativelib href="https://${host}/software/avctKVMIOWin64.jar" download="eager"/>
   <nativelib href="https://${host}/software/avctVMAPI_DLLWin64.jar" download="eager"/>
 </resources>
 <resources os="Windows" arch="x86_64">
   <nativelib href="https://${host}/software/avctKVMIOWin64.jar" download="eager"/>
   <nativelib href="https://${host}/software/avctVMAPI_DLLWin64.jar" download="eager"/>
 </resources>
  <resources os="Linux" arch="x86">
    <nativelib href="https://${host}/software/avctKVMIOLinux32.jar" download="eager"/>
   <nativelib href="https://${host}/software/avctVMAPI_DLLLinux32.jar" download="eager"/>
